@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Department </h1>
<br />
<br />
<form asp-action="Delete">
    <div class="form-group">
        <label asp-for="Code" class="col-form-label"></label>
        <input readonly asp-for="Code" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Name" class="col-form-label"></label>
        <input readonly asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="CreateAt" class="col-form-label"></label>
        <input readonly asp-for="CreateAt" class="form-control" />
    </div>
    <br />
    <div class="my-3">
    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-controller="Department" asp-action="Index" class="btn btn-light">Back To List</a>
   </div>
</form>